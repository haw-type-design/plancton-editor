{
  "extends": "eslint:recommended",
  "env": {
    "node": true
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "classes": true,
    "arrowFunctions": true,
    "modules": true,
    "defaultParams": true,
    "templateStrings": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "destructuring": true,
    "spread": true
  },
}
