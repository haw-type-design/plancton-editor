outputtemplate := "output-svg/%c.svg";
outputformat := "svg";

% global unity
u := 7pt;

% X Unity
ux := 1u;

% Y Unity
uy := 1u;

% X Stroke Unity
sx := 1u;

% Y Stroke Unity
sy := 1u;

% Stroke rotation
rot := 0;

% Display grid | 0 = off, 1 = on
grid := 0;

% Display dot label | 0 = off, 1 = on
dot_label := 0;