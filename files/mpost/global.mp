outputtemplate := "files/output-svg/%c.svg";
outputformat := "svg";

% global unity
u := 11pt;

% X Unity
ux := 0.9u;

% Y Unity
uy := 1u;

% Stroke Unity
s := 1.3u;

% X Stroke Unity
sx := 1s;

% Y Stroke Unity
sy := 1s;

% Stroke rotation
rot := 129;

% Display grid
grid := 1;

% Display dot label
dot_label := 0;