outputtemplate := "files/output-svg/%c.svg";
outputformat := "svg";

% global unity
u := 31pt;

% X Unity
ux := 1.4u;

% Y Unity
uy := 1.3u;

% Stroke Unity
s := 0.75u;

% X Stroke Unity
sx := 0.95s;

% Y Stroke Unity
sy := 0.95s;

% Stroke rotation
rot := 121;

% Display grid
grid := 1;

% Display dot label
dot_label := 0;